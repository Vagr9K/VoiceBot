{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "curly": [true, "ignore-same-line"],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^export {(.*?)}"
      }
    ]
  },
  "rulesDirectory": []
}
